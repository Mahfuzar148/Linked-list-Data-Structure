#include<bits/stdc++.h>
using namespace std;
struct myStruct{
        int age;
        string name;
        long long salary;
}s1,s2;
int main()
{
    s1.age = 10;
    s2.age = 20;

    s1.name = "Mahfuzar Rahman";
    s2.name = "Naiem ";

    s1.salary = 2000000;
    s2.salary=10000000;
    cout<<s1.age<<" "<<s1.name<<" "<<s1.salary<<endl;
     cout<<s2.age<<" "<<s2.name<<" "<<s2.salary<<endl;

     struct myStruct info[10];
     for(int i=0;i<4;i++)
     {
         cin>>info[i].age;
         cin>>info[i].name;
         cin>>info[i].salary;
     }
      for(int i=0;i<4;i++)
     {
         cout<<info[i].age<<endl;;
         cout<<info[i].name<<endl;
         cout<<info[i].salary<<endl;
     }
     return 0;
}
